{
  "builds": [
    {
      "src": "backend.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "backend.py"
    },
    {
      "src": "/admin",
      "dest": "/static/admin.html"
    },
    {
      "src": "/",
      "dest": "/static/index.html"
    }
  ]
}